{
  "private": true,
  "name": "electron-umi-project",
  "version": "1.0.0",
  "main": "./main.js",
  "description": "基于electronjs||koa2||umijs搭建的客户端项目",
  "scripts": {
    "dev:server:local": "cross-env PROCESS_ENV=development umi dev",
    "dev:server:prod": "cross-env PROCESS_ENV=production umi dev",
    "dev:client:local": "cross-env PROCESS_ENV=development electron main.js",
    "dev:client:prod": "cross-env PROCESS_ENV=production electron main.js",
    "build:mac": "electron-builder --universal",
    "build:win": "electron-builder --windows --ia32",
    "lint": "eslint {src,mock,tests}/**/*.{js,jsx} --fix",
    "ico": "png-to-ico ./build/icon.png > ./build/installerIcon.ico",
    "icns": "png2icons ./build/icon.png ./build/icon -icns -bc -i",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "antd": "4.9.1",
    "axios": "0.21.1",
    "ahooks": "2.10.9",
    "dva": "2.6.0-beta.20",
    "react": "16.8.0",
    "react-dom": "16.8.0",
    "react-router": "5.1.2",
    "react-router-dom": "5.1.2",
    "path-to-regexp": "6.2.0",
    "throttle-debounce": "^3.0.1",
    "@ant-design/pro-layout": "6.24.3",
    "@babel/core": "7.15.0",
    "babel-plugin-module-resolver": "4.1.0",
    "@babel/plugin-transform-runtime": "7.15.0",
    "@babel/preset-env": "7.15.0",
    "@babel/register": "7.15.3",
    "koa": "2.13.1",
    "@koa/router": "10.1.1",
    "koa-static": "^5.0.0"
  },
  "devDependencies": {
    "@umijs/plugin-sass": "1.1.1",
    "@umijs/preset-react": "1.x",
    "babel-eslint": "9.0.0",
    "cross-env": "7.0.3",
    "electron": "12.0.18",
    "electron-builder": "22.10.5",
    "electron-updater": "4.3.9",
    "eslint": "5.4.0",
    "eslint-config-umi": "1.4.0",
    "eslint-plugin-flowtype": "2.50.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.11.1",
    "husky": "0.14.3",
    "lint-staged": "7.2.2",
    "node-sass": "4.14.1",
    "png-to-ico": "^2.1.2",
    "png2icons": "^2.0.1",
    "react-test-renderer": "16.7.0",
    "umi": "3.5.20",
    "webpack-open-browser": "1.0.5"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
